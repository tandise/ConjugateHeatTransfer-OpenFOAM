FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
 // * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat	raw;
surfaceFormat     raw;

sets
(
    
    TatXLine
    {
        type            uniform;
        name            cut;
        axis            y;
        start           (0.5 1.2 0);
        end             (0.5 2.2 0);
        nPoints         100;
    }
);

surfaces
(
    right
    {
        type            patch;    // always triangulated
	patchName	right;        
        interpolate     true;
        triangulate false;
    }

);


fields
(
	//Usolid
	T
);


